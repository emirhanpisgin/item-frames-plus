{
	"schemaVersion": 1,
	"id": "minimalitemframes",
	"version": "${version}",
	"name": "Minimal Item Frames",
	"description": "This mod makes item frame's hitboxes smaller so you can open your chests easily!",
	"authors": [
		"Kryp Arnold"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/minimalitemframes/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.kryp.minimalitemframes.MinimalItemFrames"
		],
		"fabric-datagen": [
			"com.kryp.minimalitemframes.MinimalItemFramesDataGenerator"
		]
	},
	"mixins": [
		"minimalitemframes.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": ">=1.20",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}